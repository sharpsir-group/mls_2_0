# MLS 2.0 Environment Configuration
# Copy this file to .env and fill in your values.
# The .env file is git-ignored for security.

# ============================================
# 1. Qobrix CRM Configuration
# ============================================
# API credentials for ETL pipeline
QOBRIX_API_USER=your-qobrix-api-user-uuid
QOBRIX_API_KEY=your-qobrix-api-key
QOBRIX_API_BASE_URL=https://your-instance.qobrix.com/api/v2

# Default currency code (ISO 4217) for properties
QOBRIX_DEFAULT_CURRENCY=EUR

# ============================================
# 2. Databricks Configuration
# ============================================
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-databricks-personal-access-token
DATABRICKS_WAREHOUSE_ID=your-sql-warehouse-id
DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/your-warehouse-id
DATABRICKS_CATALOG=mls2
DATABRICKS_SCHEMA=reso_gold

# ============================================
# 3. RESO Web API Settings (optional)
# ============================================
# API_TITLE=RESO Web API
# API_VERSION=2.0.0
# MAX_PAGE_SIZE=1000
# DEFAULT_PAGE_SIZE=100
# QUERY_TIMEOUT_SECONDS=30

# 3. RESO Web API Authentication
# Comma-separated list of valid API keys
# Leave empty to disable authentication (allow all requests)
# Example: API_KEYS=key1,key2,key3
API_KEYS=
